{
    "name": "krymosoftware/php-k8s",
    "description": "Control your Kubernetes clusters with this PHP-based Kubernetes client. It supports any form of authentication, the exec API, and it has an easy implementation for CRDs.",
    "keywords": [
        "php",
        "kubernetes",
        "k8s",
        "k3s",
        "k0s",
        "cluster",
        "api",
        "kubeadm",
        "kubeapi",
        "kube"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/KrymoSoftware/php-k8s",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        },
        {
            "name": "Krymo Software",
            "email": "info@krymo.software",
            "role": "Fork Maintainer, Developer",
            "homepage": "https://krymo.software"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/macroable": "^9.35|^10.1|^11.0|^12.0",
        "illuminate/support": "^9.35|^10.1|^11.0|^12.0",
        "ratchet/pawl": "0.4.1",
        "symfony/process": "^5.4|^6.0|^7.0",
        "vierbergenlars/php-semver": "^2.1|^3.0",
        "ext-yaml": "*"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^7.23|^8.1|^9.0|^10.0",
        "phpunit/phpunit": "^9.5.20|^10.0|^11.0",
        "vimeo/psalm": "^4.20|^5.22|^6.11"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
